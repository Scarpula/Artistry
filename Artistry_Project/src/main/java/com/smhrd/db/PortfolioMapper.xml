<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smhrd.db.PortfolioMapper">

	<insert id="insertPortfolio" parameterType="Portfolio">
		insert into web_board values
		(num_board.nextval, #{title}, #{writer}, #{filename}, #{content}, sysdate)
	</insert>

	<select id="showPortfolio" resultType="Portfolio">
		select * from web_board order by b_date desc
	</select>

</mapper>